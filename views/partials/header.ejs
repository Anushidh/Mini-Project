<script>
  
  
  
    document.addEventListener('DOMContentLoaded', function() {
        var searchForm = document.getElementById('searchForm');
        var searchInput = document.getElementById('searchInput');
        var searchButton = document.getElementById('searchButton');

        searchButton.addEventListener('click', function(event) {
            event.preventDefault(); // Prevent default button click behavior

            var productName = searchInput.value.trim(); // Get the value entered in the search input field

            // Make AJAX request to searchProduct route
            var xhr = new XMLHttpRequest();
            xhr.open('POST', '/search-product');
            xhr.setRequestHeader('Content-Type', 'application/json');
            xhr.onload = function() {
                if (xhr.status === 200) {
                    var response = JSON.parse(xhr.responseText);
                    var productId = response.productId;
                    console.log(productId);
                    // Redirect to the product details page
                    window.location.href = '/productDetails/' + productId;
                } else {
                    console.error('Request failed. Status: ' + xhr.status);
                }
            };
            xhr.send(JSON.stringify({ productName: productName }));
        });
    });







</script>